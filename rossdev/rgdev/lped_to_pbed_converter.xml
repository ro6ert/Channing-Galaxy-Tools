<tool id="CONVERTER_lped_to_pbed_0" name="Convert Linkage Ped to Plink Pbed" version="1.0.0">
  <!-- <description>__NOT_USED_CURRENTLY_FOR_CONVERTERS__</description> -->
  <!-- Used on the metadata edit page. -->
  <command interpreter="python">lped_to_pbed_converter.py $input1.extra_files_path/$input1.metadata.base_name $output1 $output1.extra_files_path</command>
  <inputs>
    <param format="lped" name="input1" type="data" label="Choose Linkage format pedigree file"/>
  </inputs>
  <outputs>
    <data format="pbed" name="output1"  metadata_source="input1" />
  </outputs>
  <help>
  </help>
</tool>
